    <script>
        const form = document.getElementById("signup-form");
        const name = document.getElementById("name");
        const password = document.getElementById("password");
        const nameError = document.getElementById("name_error");
        const passwordError = document.getElementById("password_error");
        const resumeSection = document.getElementById("resume");

        form.addEventListener('submit', (e) => {
            e.preventDefault(); // Prevent form submission

            // Reset errors
            nameError.innerHTML = "";
            passwordError.innerHTML = "";

            // Check for name
            if (name.value === '') {
                nameError.innerHTML = "Name is required";
                return; // Exit the function if name is not entered
            }

            // Check for password length
            if (password.value.length <= 6) {
                passwordError.innerHTML = "Password should be more than 6 characters";
                return; 
            }
            const passwordPattern = /^(?=.*[a-z])(?=.*[A-Z])(?=.*\d)[a-zA-Z\d]{8,}$/;
            if (!passwordPattern.test(password.value)) {
                passwordError.innerHTML = "Password must contain at least one uppercase letter, one lowercase letter, one digit, and be at least 8 characters long";
                return; // Exit the function if password format is invalid
            }


            
            form.style.display = "none";
            resumeSection.style.display = "block";
        });
    </script>
